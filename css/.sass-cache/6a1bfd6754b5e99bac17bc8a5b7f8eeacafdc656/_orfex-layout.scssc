:@has_childrenT:@template"7#/* All Sections */

#branding .section, #mainwell .section {
margin: 0 auto;
max-width: 1290px;
}

.princeton-university-logo-block p {
float: left;
}

#branding p>a {
background: url("../images/princeton-university-logo.png") no-repeat 50% 50%;
background-size: 296.66667px 24.66667px;
display: block;
// height: 40px; 
text-indent: -999em;
min-width: 300px;
text-align: left;
padding: 10px 0;
}

#branding h1 a {
height: 40px;
}

.search-form {
margin-bottom: 0;
}
#block-search-form {
padding-bottom: 0;
padding-top: 0.5em;
}

ul.menu.clearfix {
list-style-type: none;
}

#main-wrapper {
margin-bottom: 1em;
}

#main {
margin-top: 0;
}

.sidebar .block {
	border-color: white;
	border: 0;
	padding: 0;
	margin: 0;
}

.region-header, .region-header a, .region-header li a.active, #name-and-slogan, #name-and-slogan a, #secondary-menu-links li a {
    color: black;
    text-align: center;
}



#site-name a {
	font-size: 1em;
	font-weight: 500;
}

#site-slogan {
	font-size: 1.2em;
	color: #666666;
}

tr, tr.odd {
	background-color: white;
}

.region-header{
	float: right;
}

#footer .block {
clear: none;
margin: 0 1em 0 1em;
/* float: left; */
}
#block-block-1 {
	margin-left: 0;
}

#footer .region {
margin-top: 0;
}

#name-and-slogan {
	margin: 0 30% 0 30%;
	width: 55%;
}

#footer-wrapper {
	padding: 0;
}

#footer {
	margin-top: 0;
}

#footer .block .content {
	margin-top: 0;
}

#main {
	margin-bottom: 0;
}

.region-diptych-first, .region-diptych-second {
display: inline;
float: left;
position: relative;
width: 50%;
}

.block ol, .block ul {
margin: 1em 0 0 0;
padding: 0;
}

.field-type-taxonomy-term-reference {
margin: 0;
}

.field-name-field-speaker {
	font-size: 1.8em;
}

.field-name-field-institution {
	font-size: 1.4em;
}

td {
white-space: nowrap;
}

tr td, tr th {
padding: 0 10px 0 10px;
}

h1, h2, h3, h4, h5, h6 {
margin: 0.5em 0 0.5em 0;
}

/* Home Page */

.page-home .tabs {
display: none;
}

#triptych h2 {
margin-bottom: 1em;
}

ul.menu li {
margin: 0.5em 0;
}

/* Home Page - Event List */

.view-display-id-upcoming_conferences .item-list ul li {
	margin-bottom: 0.25em;
}

/* Home Page - Main Menu */

.menu-block-18.menu-name-main-menu ul.menu li a {
padding: 0 0.5em 0 0;
color: white;
font-size: 1.1em;
}

.menu-block-18.menu-name-main-menu ul.menu li a.active {
color: rgb(201, 201, 201);
}

.menu-block-18.menu-name-main-menu ul li {
display: inline;
vertical-align: middle;
}

.menu-block-18.menu-name-main-menu ul.menu {
	margin: 0;
	padding: 0.5em 0 0.5em 0;
	text-align: center;
}

.menu-block-18.menu-name-main-menu ul>li {
	margin:0;
	padding:0;
}

.region-headerwell .menu-block-18.menu-name-main-menu {
	background-color: rgba(129, 130, 133, 1);
	width: 100%;
}

.view-display-id-frontpage_table td {
        vertical-align: top;
}

/* Home Page - Administrative Menu */

#block-menu-block-19 {
border: 1px solid #be7;
}

#block-menu-block-19 h2 {
display:none;
}

.menu-block-19.menu-name-navigation ul.menu li a {
padding: 0 0.5em 0 0;
color: gray;
font-size: 1.1em;
}

.menu-block-19.menu-name-navigation ul.menu li a.active {
color: rgb(81, 38, 6);
}

.menu-block-19.menu-name-navigation ul li {
display: inline;
vertical-align: middle;
}

.menu-block-19.menu-name-navigation ul.menu {
        margin: 0;
        padding: 0.5em 0 0.5em 0;
        text-align: center;
}

.menu-block-19.menu-name-navigation ul>li {
        margin:0;
        padding:0;
}

.region-headerwell .menu-block-19.menu-name-navigation {
        background-color: rgb(248, 255, 241);
        width: 100%;
}

.view-display-id-frontpage_table td {
        vertical-align: top;
}


/* Home Page - Content */

#carmona-welcome {
float: left;
width: 20%;
margin: 0 1em 1em 0;
}

/* Home Page - Header */

.block-search {
float: right;
}

#logo {
max-width: 45%;
min-width: 350px;
}

/* Home Page - Triptych */

.views-row {
margin: 0.25em 0 0.25em 0;
}

.view-research-areas-list .view-header {
text-align:center;
}

.news.home .view-header {
margin-bottom: 1em;
padding-bottom: 1em;
border-bottom: 1px solid #dfdfdf;
line-height: 1.3;
}

/* Home Page - Spotlight */
.views-slideshow-controls-bottom {
display: table;
margin: 0 auto;
}

#highlighted {
border-bottom: none;
margin-top: 0.75em;
}

.view-display-id-spotlight_block {
.view-header {
    position: absolute;
    background-color: rgba(255, 255, 255, 0.70196);
    z-index: 200;
    font-size: 1.5em;
    padding: 0 1em 0 1em;
}
.views-field-title {
    position: absolute;
    background-color: rgba(255, 255, 255, 0.70196);
    z-index: 200;
    font-size: 1.5em; 
    margin: -1.7em 1em 0 0;
    width: 100%;
}

/* Home Page - Slideshow */
//Respond to mobile.

@media screen and (max-width: 1024px){
    .views-field-title {
        font-size:1.25em; }

}

@media screen and (max-width: 768px) {
	.views-field-title {
	font-size: 1.1em;
	position: relative;
	margin: 0 auto;
	z-index: 0;
	background-color:white;	
}
	.view-header {
	position:relative;
	background-color:white;
	z-index: 0;
	font-size: 1.25em;
	}

}
}

/* Social Media Icons */
#block-block-19 {
float: right;
}

/* People Section */

.faculty-contact-card, .contact-card {
  float: left;
  list-style: none;
  width: 48%;
/*  padding: 0 0 5% 0 !important; */


@media screen and (max-width: 1024px) {
float: none;
list-style: none;
width: 100%;
padding: 0 0 5% 0 !important;
}

}

.faculty-contact-card {
min-height: 13em;
}

.contact-card {
  min-height: 10em;
}

.page-people .item-list ul li .views-field-picture {
  float: left;
  padding: 0 0.5em 0 0;
}

.view-people.faculty .views-field-field-researchicon ul li {
  clear: none;
  float: left;
  list-style-type: none;
  margin: 0 0.25em 0 0.25em;
  padding: 0;
}

/* Atrium - Spotlight, Events, Faculty Slideshow */

.page-atrium-spotlight, .page-events-atrium, .page-people-faculty-slideshow {

#header div.section, #featured div.section, #messages div.section, #main, #triptych, #footer-columns, #footer {
	max-width: 1920px;
}

	#headerwell {
	display: none;
	}
	#mainwell {
	display: none;
	}
	#messages {
	display: none;
	}
	#footer-wrapper {
	display: none;
	}
	#search-block-form {
	display: none;
	}

}

/* Atrium - Events */

.page-events-atrium {
	font-size: 1.3em;
}

/* Atrium - Spotlight */

.page-atrium-spotlight {
        h1#page-title {
        position: absolute;
        color: #0090e4;
        background-color: rgba(255, 255, 255, 0.70196);
        z-index: 200;
        font-size: 1.75em;
        padding: 1em;
        }
        .views-field.views-field-title {
        position: absolute;
        background-color: rgba(255, 255, 255, 0.70196);
        color: #0090e4;
        z-index: 200;
        font-size: 1.75em;
        margin: -1.7em 1em 0 0;
        width: 100%;
        }
}

/* Atrium - Faculty Slideshow */

.page-people-faculty-slideshow {

        h1#page-title {
        position: absolute;
        color: #0090e4;
        background-color: rgba(255, 255, 255, 0.70196);
        z-index: 200;
        font-size: 1.75em;
        padding: 0.5em;
        }

	#header {
	display: none;
	}

	#main-wrapper {
	background: #161617;
	margin-bottom: 0;
	}

/* Fix this mess. */
	div#views_slideshow_cycle_div_people-page_1_4.views-slideshow-cycle-main-frame-row.views_slideshow_cycle_slide.views_slideshow_slide.views-row-5.views_slideshow_cycle_hidden.views-row-odd, div#views_slideshow_cycle_div_people-page_1_3.views-slideshow-cycle-main-frame-row.views_slideshow_cycle_slide.views_slideshow_slide.views-row-4.views_slideshow_cycle_hidden.views-row-even, div#views_slideshow_cycle_div_people-page_1_2.views-slideshow-cycle-main-frame-row.views_slideshow_cycle_slide.views_slideshow_slide.views-row-3.views_slideshow_cycle_hidden.views-row-odd, div#views_slideshow_cycle_teaser_section_people-page_1.views-slideshow-cycle-main-frame.views_slideshow_cycle_teaser_section, div#views_slideshow_cycle_div_people-page_1_1.views-slideshow-cycle-main-frame-row.views_slideshow_cycle_slide.views_slideshow_slide.views-row-2.views_slideshow_cycle_hidden.views-row-even {
	overflow: visible;
	background: white;
	color: gray;
	text-align:center;
	width: 800px !important;
	}

	.views-slideshow-cycle-main-frame-row-item {
  	float: left;
	padding: 0.1em;
}

	.views-row {
	width: 245px;
	height: 500px;
	}

}

/* Event Page */

ul.event>li {
margin-bottom: 1em;
}

/* Advisory Council */

.view-advisory-council .views-row {
margin: 2em 0 1em 0;
}

.advisory-council-member-photo {
padding: 0.25em;
}

/* Academics - Academic Jobs */
.view-display-id-academic_jobs_page .views-row, .page-about-employment .view-id-employment .views-row {
  margin: 2em 0 2em 0;
  padding: 0 0 2em 0;
  border-bottom: 1px solid gray; }
.view-display-id-academic_jobs_page .views-field-title, .page-about-employment .view-id-employment .views-field-title {
  text-decoration: none;
  margin: 0 0 1em 0; }
.view-employment .views-field-title {
  font-size: 1.2em; }

/* Academics - Job Placement */

.view-graduate-employment {
ul li {
list-style-type: none;
}
.item-list ul li {
margin-bottom: 25px;
}
}
