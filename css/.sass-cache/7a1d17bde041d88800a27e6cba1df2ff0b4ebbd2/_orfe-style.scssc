:@has_childrenT:@template"9$* {
  font-family: 'Verdana', sans-serif;
}

body {
  font-size: 62.5%;
  font-family: 'Verdana', serif;
}

#site-slogan, .ui-widget, .comment-form label {
  font-family: 'Verdana', serif;
}

table {
  font-size: 0.9em;
}

.page-admin, .page-node-webform-results {
	table {
  font-size: 1.2em;
	}
}

tbody {
  border: none;
}

tr {
  &.even, &.odd {
    border: none;
  }
}

input, textarea {
  font-size: 1.1em;
  padding: 0;
  line-height: 1.8em;
}

table tr th {
  font-weight: normal;
  a {
    font-weight: normal;
    &:hover {
      font-weight: normal;
    }
  }
}

fieldset {
  margin: 0 0 2.5em 0;
  padding: 0.5em;
  border-radius: 0;
  legend {
    text-shadow: none;
    text-indent: 0.25em;
  }
}

#main-menu-links li a:hover {
  color: #F7941D;
}

li {
  &.expanded, &.collapsed, &.leaf {
    padding: 0;
    margin: 0;
  }
}

.block {
  ol, ul {
    margin: 0;
    padding: 0;
  }
}

.field-name-body {
  ol, ul {
    padding: 0 0 1em 1.5em;
  }
  li > {
    ol, ul {
      padding: 0.25em 0 1em 1.5em;
    }
  }
}

#header {
  font-size: 1.4em;
}

.sidebar .block .content {
  font-size: 1.2em;
  line-height: 1.6em;
}

h1, .block h2 {
  font-size: 1.7em;
  border-bottom: none;
}

#content {
  h2 {
    margin: 0 0 1em 0;
    color: rgb(252, 152, 3);
  }
  .field-name-body h2 {
    font-size: 1.2em;
  }
}

.block.view-content {
  font-size: 1em;
}

.view-content, .node .content {
  font-size: 1.2em;
}

.view-empty {
  font-size: 1em;
}

.field-name-field-institution li {
  color: #999;
}

#search-form input#edit-keys, #block-search-form .form-item-search-block-form input {
  float: left;
  font-size: 1em;
  width: 80%;
}

tr {
  td, th {
    padding: 0.4em 0.2em;
    border: 0.09em solid #fff;
    text-align: left;
  }
}

.home! .about .message p {}

p {
  line-height: 1.6em;
  margin: 0 0 1.75em 0;
}

.node! .content {}

#header {
  border-bottom: 0.1em solid #e2e2e2;
}

.region-header {
  border: 0;
}

/* Remove the border bottom included in Bartik. */

#highlighted {
  border-bottom: none;
}

/* Harmonize the H1 styling to be consistent with the page titles and front page section titles. */

h1 {
  color: rgb(249, 149, 36);
  &#page-title {
    color: rgb(249, 149, 36);
  }
  font-family: 'Verdana', sans-serif;
}

h2, h3, h4, h5 {
  font-family: 'Verdana', sans-serif;
}

#main-menu-links {
  li {
    margin: 0 0.85em;
  }
  a {
    text-shadow: none;
    border-radius: 0;
  }
}

#search-form .form-item-keys label {
  display: none;
}

/* Research area titles and icons on the Home page. */

/* Style main menu to look like tabs at desktop width. */

#main-menu-links li a {
  &.active-trail, &.active {
    color: rgb(249, 149, 36);
    border-top: .1em solid #e2e2e2;
    border-right: 0.1em solid #e2e2e2;
    border-bottom: 0.1em solid #fff;
    border-left: 0.1em solid #e2e2e2;
    margin-bottom: -0.1em;
  }
}

#menu-toggle {
  color: rgb(249, 149, 36);
  border-top: .1em solid #e2e2e2;
  border-right: 0.1em solid #e2e2e2;
  border-bottom: 0.1em solid #fff;
  border-left: 0.1em solid #e2e2e2;
  margin-bottom: -0.1em;
}

/* Set the featured area, which displays the logged in and access denied messages, to display with a plain background and no margins. */

.featured #main-menu-links li a {
  &:active, &.active {
    background: white;
  }
}

#featured, #messages {
  line-height: 1.6em;
  background: white;
  border-bottom: none;
  padding: 0;
  text-align: left;
  font-size: 1.4em;
}

#featured .content {
  margin: 0;
}

#footer-wrapper {
  color: #c0c0c0;
}

#footer-wrapper a:hover, #footer-wrapper a:focus {
  color: #0070b3;
}

.sidebar h2, .sidebar-second h2 {
  padding: 0 0 0.5em 0;
  color: rgb(249, 149, 36);
}

ul li {
  &.collapsed, &.leaf {
    list-style-image: none;
    list-style-type: none;
  }
}

.home.news {
  .highlighted-news-headline, .news-headline {
    /* margin: 0 0 0.55em 0; */
  }
  .highlighted-news-headline.views-row-last {
    margin: 0 0 1em 0;
  }
  .news-headline.views-row-first {
    margin: 1em 0 0.5em 0;
  }
  .view-header {
    border-bottom: 1px solid #ccc;
  }
}

.event_daytime, .event_location {
  color: #999;
  font-style: italic;
}

.home.events ul {
  padding-left: 0;
  list-style-type: none;
}

.feed-icon img {
  display: none;
}

.item-list! ul li {}

ul.menu li {
  a {
    padding: 0;
  }
  margin: 0 0 0.25em 0;
}

.node-dynamic-events-page {
  .event li {
    list-style-type: none;
  }
  ul {
    margin: 0;
    padding: 0;
  }
}

.view-news {
  .views-field-title {
    font-size: 1.2em;
  }
  .views-field-created {
    color: #999;
    margin: 0 0 1em 0;
  }
}

.news .home .news-headline {
  font-size: 0.8em;
  margin: 0 0 0.5em 0;
}

.view-employment .views-field-title {
  font-size: 1.2em;
}

.view-highlighted-news .highlighted-news-headline {
  font-size: 0.8em;
  margin: 0 0 0.5em 0;
}

.sidebar .menu h2, .sidebar-second .menu h2 {
  display: none;
}

@media (min-width: 570px) and (max-width: 901px) {
  .sidebar .menu h2, .sidebar-second .menu h2 {
    display: block;
  }
}

#logo img {
max-width: 444px;
}

/* Retina Macs */
@media only screen and (min--moz-device-pixel-ratio: 2),
only screen and (-o-min-device-pixel-ratio: 2/1),
only screen and (-webkit-min-device-pixel-ratio: 2),
only screen and (min-device-pixel-ratio: 2) {
#logo img {
max-width: 444px;
}
}

/* Retina iPhones */
@media only screen and (-webkit-min-device-pixel-ratio: 1.5),
only screen and (min-device-pixel-ratio: 1.5) {
#logo img {
max-width: 310px;
}
}


.research .menu li {
  list-style-image: none;
  list-style-type: none;
}

.researcharea.name {
  text-transform: capitalize;
}

.breadcrumb {
  display: none;
}

.page-admin .breadcrumb {
display: block;
font-size: 1.2em;
}

.research.faculty-research-interests .content .view .view-content .item-list {
  ul {
    padding: 0;
  }
  li {
    list-style-type: none;
    margin: 0 0 1em 0;
  }
}

.home.about.message .content .view .view-content .views-row .views-field-title .field-content h1 {
  font-size: 1.4em;
  margin: 0 0 1em 0;
}

.featured #messages {
  background: white;
}

.tabs {
  font-family: 'Verdana',sans-serif;
  font-size: 1.15em;
  ul.primary li {
    margin: 0 0.2em 0 0.2em;
  }
}

#search-form input#edit-keys, #block-search-form .form-item-search-block-form input {
  margin: 0.5em 0.5em 0.5em 0;
}

.one-sidebar.sidebar-first.page-node #main #content h2 {
  /* margin: 0.5em 0 0.5em 0; */
}

.page-research .researcharea {
  &.icon {
    max-width: 27px;
    float: left;
    margin: 0 auto;
    list-style-type: none;
    padding: 0 0.75em 0 0.75em;
    min-height: 3em;
    display: table-cell;
    vertical-align: middle;
  }
  &.description {
    display: block;
    clear: both;
  }
}

#footer-wrapper a {
  color: inherit;
}

dl {
  font-size: 1.2em;
}

.view-header {
  /* Too broad to correct the font size issue with view headers here. This impacts the sidebar-second. */
}

.sidebar h2, #featured {
  text-shadow: none;
}

.node-form {
  label, .fieldset-legend {
    font-size: 1.2em;
    font-weight: normal;
  }
}

input.form-submit {
  font-size: 1.2em;
  padding: 0.25em 1.2em 0.25em 1.2em;
}

a {
  &.button {
    font-size: 1.2em;
    margin: 1em 0 1em 0;
    padding: 0.25em 1.2em 0.25em 1.2em;
  }
  &.tabledrag-handle .handle {
    margin: -0.75em 0 0 1em;
    padding: 0.75em;
  }
}

.vertical-tabs ul.vertical-tabs-list .selected strong {
  font-size: 1.2em;
}

.form-item {
  margin: 1em 0 2em 0;
  .description {
    font-size: 0.9em;
    margin: 0 0 1em 0;
    color: #999;
  }
}

.field-multiple-table td {
  padding: 0 1em 0 1em;
}

.tabledrag-toggle-weight-wrapper {
  display: none;
}

fieldset .fieldset-wrapper, .fieldset-wrapper {
/*  padding: 1em 0 0 0; */
  margin: 0;
}

.container-inline-date {
  > .form-item {
    margin-bottom: 0;
  }
  .date-padding {
    padding: 1em 0 0 1em;
  }
}

.tips {
  font-size: 1em;
}

ul.tips {
  padding: 1em 0 1em 2em;
}

.text-format-wrapper .description {
  font-size: 1em;
  padding: 0.25em 0 1em 0;
}

.region-help {
  font-size: 1.2em;
  border: none;
  margin: 0;
  padding: 0;
}

.field-type-taxonomy-term-reference {
  margin: 0;
}

#field-collection-item-form {
  font-size: 1.4em;
}

h1#page-title {
  font-size: 1.7em;
  line-height: 1.2em;
}

div.messages {
  border: none;
}

.block-views {
  margin: 0 0 2em 0;
  h2! {}
}

.meta {
  font-size: 1.2em;
  color: #999;
  margin: -1em 0 1em 0;
}

.home.view-news-block .Job.Posting {
  font-size: 0.85em;
/*  background-color: rgb(240,240,240); */
  margin: 0 0 0.5em 0;
}

.home.view-news-block .view-employment .view-content {
  background-color: rgb(240,240,240);
}

.page-home {
	.block-menu-block.administrative {
		font-size: 1.2em;
		margin: 0 0 2em 0;

	li {
		float: left;
		margin: 0 auto;
		list-style-type: none;
		margin: 0 1em 0 1em;
		display: table-cell;
		vertical-align: middle; 
	}

	}
}

.page-navigation404 .block-system {
	font-size: 1.2em; 
}

#user-profile-form .form-item label {
	font-size: 1.2em;
}

.confirmation, .confirmation .form-item label {
	font-size: 1.2em;
}

.view-user-profiles table, .view-unpub table {
	font-size: 1em;
	min-width: 35%;
	width: auto;
	th, td {
	padding: 0.5em;
	}
}

#footer li a {
padding: 0 0.5em;
border-color: rgba(143, 143, 143, 0.15);
}

.jcbx-glry-classic {
background: white;
}
