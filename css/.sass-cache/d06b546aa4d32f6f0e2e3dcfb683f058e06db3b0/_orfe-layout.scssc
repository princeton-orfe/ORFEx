:@has_childrenT:@template">#main {
  margin-top: 0;
  margin-bottom: 0;
}

#main-wrapper {
  margin: 2em 0 0 0;
}

div.messages {
  margin: 0.5em 0 0 0;
}

@media (max-width: 900px) {
  #logo {
    padding: 1em 0 1em 0.7em;
  }
}

@media (min-width: 901px) {
  #logo {
    padding: 1em 0 1.5em 0.7em;
  }
  .region-header {
    margin: 0;
  }
}

.pu_header_logo {
  float: right !important;
  margin: 0 !important;
  padding: 1em 0 0 0 !important;
}

#header div.section, #featured div.section, #messages div.section, #main, #triptych, #footer-columns {
  max-width: 960px;
}

#footer {
  max-width: 960px;
  margin-top: 0;
  .region {
    margin-top: 0;
  }
  .block {
    margin: 0;
  }
}

h1, h2, h3, h4, h5 {
  margin: 0 0 1em 0;
}

#main-menu-links {
  padding: 0 1em;
  width: 73%;
  margin: 0 auto;
}

/* Main menu responsiveness. */

.home.photo {
  margin-bottom: 1em;
}

#content .section, .sidebar .section {
  padding: 0 1em 0 1em;
}

.content {
  /* margin: 0.75em 0 0 0; */
  margin: 0;
}

.tabs {
  margin-bottom: 1.25em;
}

.sidebar .block {
  border: 0;
  padding: 0;
  margin: 0 0 0.5em 0;
  background: white;
}

.block.block-block.home.events! {}

.viewall {
  padding-top: 0.5em;
  text-align: right;
}

@media (min-width: 851px) {
  .one-sidebar #content {
    width: 70%;
  }
  #sidebar-second {
    width: 30%;
    margin-left: -30%;
    clear: none;
  }
}

@media (max-width: 851px) {
  .one-sidebar #content {
    width: 100%;
  }
}

.sidebar-second #content {
  margin-right: 30%;
  margin-left: 0;
}

@media (min-width: 851px) {
  .sidebar-second #content {
    margin-right: 30%;
    margin-left: 0;
  }
}

#sidebar-second {
  width: 30%;
  margin-left: -30%;
}

ul.event {
  margin: 0 0 1em 0;
}

#sidebar-first {
  margin-left: -95%;
}

.menu-block-wrapper.menu-name-main-menu ul li {
  list-style-type: none;
  list-style-image: none;
}

.view-display-id-academic_jobs_page, .page-about-employment .view-id-employment {
  .views-row {
    margin: 2em 0 2em 0;
    padding: 0 0 2em 0;
    border-bottom: 1px solid gray;
  }
  .views-field-title {
    text-decoration: none;
    margin: 0 0 1em 0;
  }
  .views-field-field-related-url! {}
}

.node-document .field-name-field-description {
  margin: 0 0 1em 0;
}

/* Research area icons on the Home page. */

ul.research-area-items {
  margin: 0 0 0 -0.75em;
}

.research-area-items {
  li {
    float: left;
    margin: 0 auto;
    list-style-type: none;
    padding: 0 0.75em 0 0.75em;
    min-height: 3em;
    display: table-cell;
    vertical-align: middle;
  }
  .views-field-field-image {
    float: left;
    padding: 0 0.25em 0 0;
  }
  .views-field-name {
    text-transform: capitalize;
    float: left;
    max-width: 7em;
  }
}

/* End Research Area section under the Home tab. */

/* Faculty page under the People tab. */

.faculty-contact-card, .contact-card {
  float: left;
  list-style: none;
  width: 48%;
  padding: 0 0 5% 0 !important;
}

.faculty-contact-card {
  min-height: 15em;
}

.contact-card {
  min-height: 10em;
}

.page-people .item-list ul li .views-field-picture {
  float: left;
  padding: 0 0.5em 0 0;
}

.view-people.faculty .views-field-field-researchicon ul li {
  clear: none;
  float: left;
  list-style-type: none;
  margin: 0 0.25em 0 0.25em;
  padding: 0;
}

.advisory-council-member-photo {
  float: right;
  padding: 0 0 2em 2em;
}

.advisory-council-member-title {
  color: #999;
  margin: 0 0 1em 0;
}

.advisory-council-member-bio {
  margin: 0 0 1em 0;
}

@media (min-width: 755px) and (max-width: 907px) {
  #main-menu-links {
    width: 80%;
    li {
      width: auto;
    }
    a {
      float: left;
      padding: 0.7em 0.8em;
      margin-bottom: 0;
      border-bottom-left-radius: 0;
      border-bottom-right-radius: 0;
    }
  }
  #sidebar-first {
    margin-left: 0;
  }
  .home.researchareas {
    display: none;
  }
}

@media (min-width: 570px) and (max-width: 753px) {
  #main-menu-links {
    width: 100%;
    li {
      width: auto;
    }
    a {
      float: left;
      padding: 0.5em 0.6em;
      margin-bottom: 0;
      border-bottom-left-radius: 0;
      border-bottom-right-radius: 0;
    }
  }
  .region-sidebar-first {
    ul li {
      margin: 0 0 0.5em 0;
    }
    .expanded ul li {
      margin: 0.5em 0 0 0;
    }
  }
  #sidebar-first {
    margin-left: 0;
  }
  .home.researchareas {
    display: none;
  }
}

@media (max-width: 570px) {
  .home.photo {
    display: none;
  }
}

@media (max-width: 570px) {
  a#menu-toggle {
    display: block;
    text-shadow: none;
    font-size: 1.3em;
    border-bottom: 1px solid #ccc;
    border-top: 1px solid #ccc;
  }
  #main-menu! {}
  #main-menu-links {
    display: none;
    height: auto;
  }
  ul#main-menu-links {
    width: 100%;
  }
  #main-menu-links {
    li {
      margin: 0;
    }
    a {
      font-size: 1.3em;
      margin: 0;
    }
  }
}

@media (min-width: 570px) and (max-width: 907px) {
  margin: 0;
  float: none;
}

@media only print, (max-width: 700px) {
  .pu_header_logo {
    display: none;
  }
  .region-header {
    margin: 0;
    float: none;
  }
}

@media (max-width: 570px) {
  #main-menu-links {
    margin: 0;
    padding: 0;
    li a {
      &.active-trail, &.active {
        color: rgb(249, 149, 36);
        border-top: .1em solid #ccc;
        border-right: 0.1em solid #ccc;
        border-bottom: 0.1em solid #ccc;
        border-left: 0.1em solid #ccc;
        margin-bottom: 0;
      }
    }
  }
  #sidebar-first {
    margin-left: 0;
  }
  .sidebar .menu h2, .sidebar-second .menu h2 {
    display: block;
    padding: 1em 0 0.5em 0;
  }
  .page-people .item-list ul li .views-field-picture {
    float: none;
  }
  #sidebar-second {
    width: 100%;
    margin: 0 0 0 0;
  }
  .home.researchareas {
    display: none;
  }
  #sidebar-first {
    clear: both;
  }
}

@media (min-width: 851px) {
  #sidebar-first {
    margin-left: -95%;
  }
  .sidebar-first .menu h2 {
    display: none;
  }
}

@media (min-width: 851px) {
  .region-footer-firstcolumn {
    width: 70%;
  }
  .region-footer-fourthcolumn {
    width: 30%;
    padding: 1.5em 0 0 1em;
  }
  #footer-wrapper {
    margin: 1.05em 0 0 0;
    padding: 0;
    .block {
      margin: 1.05em 0 0 0;
      padding: 0;
    }
  }
}

li > ul.menu {
  margin: 0.5em 0 0 1em;
}

.faculty-research-interests h2 {
  margin: 1em 0 1em 0;
}

#auth_box {
  padding: 0;
  h1#the_logo {
    padding: 1em 0 1em 0;
    img {
      max-width: 100%;
    }
  }
  .form-item {
    font-size: 1.2em;
  }
}

@media (min-width: 975px) {
  #auth_box {
    width: 35%;
    max-width: 500px;
  }
}

#edit-actions input {
  font-size: 1.2em;
}

dd {
  margin: 0 0 1em 0;
}

/* Atrium slideshow */

@media (max-height: 570px) {
  .page-people-faculty-slideshow {
    #header, #sidebar-first, #featured, h1, #footer-wrapper {
      display: none;
    }
    #main {
      max-width: 1280px;
    }
    #content {
      margin: 0;
    }
  }
  body.page-people-faculty-slideshow {
    background-color: black;
  }
  .page-people-faculty-slideshow {
    #main-wrapper, #skip-link, #page-wrapper {
      background-color: black;
    }
  }
  .view_row {
    margin: 0 0 3em 0;
  }
  .page-people-faculty-slideshow #main-wrapper {
    margin: 0;
  }
  .views-slideshow-cycle-main-frame-row-item {
    color: white;
    font-size: 1.2em;
    &.views-row {
      margin: 1.2em 0 1.2em 0;
    }
  }
}

@media (min-width: 851px) {
  .external-job-sites-block {
    display: none;
  }
}

.page-field-collection-field-timeslots .tabs {
  display: none;
}

.page-research .researcharea.name {
	padding: 0.5em 0 0 0;
}

.page-displays-undergraduate {
	table {
	table-layout: fixed;
	}
	td {
	padding: 1em;
	}
	tr td, tr th {
	vertical-align: top;
	}
}

@media (max-height: 720px) {
.page-displays-undergraduate {
	#main-menu {
	display: none;
	}
	#footer-wrapper {
	display: none;
	}
	tr td, tr th {
	vertical-align: top;
	}
	table {
	table-layout: fixed;
	}
	tr, tr.even {
	background: none;
	}
}
}

.view-news-block.home .view-employment {
margin-bottom: 0.5em;
}
