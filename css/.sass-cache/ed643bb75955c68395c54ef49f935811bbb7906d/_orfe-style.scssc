:@has_childrenT:@template"Ý#* {
  font-family: 'Verdana', sans-serif;
}

body {
  font-size: 62.5%;
  font-family: 'Verdana', serif;
}

#site-slogan, .ui-widget, .comment-form label {
  font-family: 'Verdana', serif;
}

table {
  font-size: 0.9em;
}

.page-admin, .page-node-webform-results {
	table {
  font-size: 1.2em;
	}
}

tbody {
  border: none;
}

tr {
  &.even, &.odd {
    border: none;
  }
}

input, textarea {
  font-size: 1.1em;
  padding: 0;
  line-height: 1.8em;
}

table tr th {
  font-weight: normal;
  a {
    font-weight: normal;
    &:hover {
      font-weight: normal;
    }
  }
}

fieldset {
  margin: 0 0 2.5em 0;
  padding: 0.5em;
  border-radius: 0;
  legend {
    text-shadow: none;
    text-indent: 0.25em;
  }
}

#main-menu-links li a:hover {
  color: #F7941D;
}

li {
  &.expanded, &.collapsed, &.leaf {
    padding: 0;
    margin: 0;
  }
}

.block {
  ol, ul {
    margin: 0;
    padding: 0;
  }
}

.field-name-body {
  ol, ul {
    padding: 0 0 1em 1.5em;
  }
  li > {
    ol, ul {
      padding: 0.25em 0 1em 1.5em;
    }
  }
}

#header {
  font-size: 1.4em;
}

.sidebar .block .content {
  font-size: 1.2em;
  line-height: 1.6em;
}

h1, .block h2 {
  font-size: 1.7em;
  border-bottom: none;
}

#content {
  h2 {
    margin: 0 0 1em 0;
    color: rgb(252, 152, 3);
  }
  .field-name-body h2 {
    font-size: 1.2em;
  }
}

.block.view-content {
  font-size: 1em;
}

.view-content, .node .content {
  font-size: 1.2em;
}

.view-empty {
  font-size: 1em;
}

.field-name-field-institution li {
  color: #999;
}

#search-form input#edit-keys, #block-search-form .form-item-search-block-form input {
  float: left;
  font-size: 1em;
  width: 80%;
}

tr {
  td, th {
    padding: 0.4em 0.2em;
    border: 0.09em solid #fff;
    text-align: left;
  }
}

.home! .about .message p {}

p {
  line-height: 1.6em;
  margin: 0 0 1.75em 0;
}

.node! .content {}

#header {
  border-bottom: 0.1em solid #e2e2e2;
}

.region-header {
  border: 0;
}

/* Remove the border bottom included in Bartik. */

#highlighted {
  border-bottom: none;
}

/* Harmonize the H1 styling to be consistent with the page titles and front page section titles. */

h1 {
  color: rgb(249, 149, 36);
  &#page-title {
    color: rgb(249, 149, 36);
  }
  font-family: 'Verdana', sans-serif;
}

h2, h3, h4, h5 {
  font-family: 'Verdana', sans-serif;
}

#main-menu-links {
  li {
    margin: 0 0.85em;
  }
  a {
    text-shadow: none;
    border-radius: 0;
  }
}

#search-form .form-item-keys label {
  display: none;
}

/* Research area titles and icons on the Home page. */

/* Style main menu to look like tabs at desktop width. */

#main-menu-links li a {
  &.active-trail, &.active {
    color: rgb(249, 149, 36);
    border-top: .1em solid #e2e2e2;
    border-right: 0.1em solid #e2e2e2;
    border-bottom: 0.1em solid #fff;
    border-left: 0.1em solid #e2e2e2;
    margin-bottom: -0.1em;
  }
}

#menu-toggle {
  color: rgb(249, 149, 36);
  border-top: .1em solid #e2e2e2;
  border-right: 0.1em solid #e2e2e2;
  border-bottom: 0.1em solid #fff;
  border-left: 0.1em solid #e2e2e2;
  margin-bottom: -0.1em;
}

/* Set the featured area, which displays the logged in and access denied messages, to display with a plain background and no margins. */

.featured #main-menu-links li a {
  &:active, &.active {
    background: white;
  }
}

#featured, #messages {
  line-height: 1.6em;
  background: white;
  border-bottom: none;
  padding: 0;
  text-align: left;
  font-size: 1.4em;
}

#featured .content {
  margin: 0;
}

#footer-wrapper {
  color: #c0c0c0;
}

#footer-wrapper a:hover, #footer-wrapper a:focus {
  color: #0070b3;
}

.sidebar h2, .sidebar-second h2 {
  padding: 0 0 0.5em 0;
  color: rgb(249, 149, 36);
}

ul li {
  &.collapsed, &.leaf {
    list-style-image: none;
    list-style-type: none;
  }
}

.home.news {
  .highlighted-news-headline, .news-headline {
    /* margin: 0 0 0.55em 0; */
  }
  .highlighted-news-headline.views-row-last {
    margin: 0 0 1em 0;
  }
  .news-headline.views-row-first {
    margin: 1em 0 0.5em 0;
  }
  .view-header {
    border-bottom: 1px solid #ccc;
  }
}

.event_daytime, .event_location {
  color: #999;
  font-style: italic;
}

.home.events ul {
  padding-left: 0;
  list-style-type: none;
}

.feed-icon img {
  display: none;
}

.item-list! ul li {}

ul.menu li {
  a {
    padding: 0;
  }
  margin: 0 0 0.25em 0;
}

.node-dynamic-events-page {
  .event li {
    list-style-type: none;
  }
  ul {
    margin: 0;
    padding: 0;
  }
}

.view-news {
  .views-field-title {
    font-size: 1.2em;
  }
  .views-field-created {
    color: #999;
    margin: 0 0 1em 0;
  }
}

.news .home .news-headline {
  font-size: 0.8em;
  margin: 0 0 0.5em 0;
}

.view-employment .views-field-title {
  font-size: 1.2em;
}

.view-highlighted-news .highlighted-news-headline {
  font-size: 0.8em;
  margin: 0 0 0.5em 0;
}

.sidebar .menu h2, .sidebar-second .menu h2 {
  display: none;
}

@media (min-width: 570px) and (max-width: 901px) {
  .sidebar .menu h2, .sidebar-second .menu h2 {
    display: block;
  }
}

/* @media only screen and (-webkit-min-device-pixel-ratio: 1.5), only screen and (min-device-pixel-ratio: 1.5) {
	#logo img {
	display: none;
	}
	#logo {
	background-image: url('http://orfe.princeton.edu/sites/orfe.princeton.edu/themes/responsive_ORFE/logo@2x.png');
