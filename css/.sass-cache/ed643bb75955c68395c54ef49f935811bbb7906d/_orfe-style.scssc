.research .menu li {
  list-style-image: none;
  list-style-type: none;
}

.researcharea.name {
  text-transform: capitalize;
}

.breadcrumb {
  display: none;
}

.page-admin .breadcrumb {
display: block;
font-size: 1.2em;
}

.research.faculty-research-interests .content .view .view-content .item-list {
  ul {
    padding: 0;
  }
  li {
    list-style-type: none;
    margin: 0 0 1em 0;
  }
}

.home.about.message .content .view .view-content .views-row .views-field-title .field-content h1 {
  font-size: 1.4em;
  margin: 0 0 1em 0;
}

.featured #messages {
  background: white;
}

.tabs {
  font-family: 'Verdana',sans-serif;
  font-size: 1.15em;
  ul.primary li {
    margin: 0 0.2em 0 0.2em;
  }
}

#search-form input#edit-keys, #block-search-form .form-item-search-block-form input {
  margin: 0.5em 0.5em 0.5em 0;
}

.one-sidebar.sidebar-first.page-node #main #content h2 {
  /* margin: 0.5em 0 0.5em 0; */
}

.page-research .researcharea {
  &.icon {
    max-width: 27px;
    float: left;
    margin: 0 auto;
    list-style-type: none;
    padding: 0 0.75em 0 0.75em;
    min-height: 3em;
    display: table-cell;
    vertical-align: middle;
  }
  &.description {
    display: block;
    clear: both;
  }
}

#footer-wrapper a {
  color: inherit;
}

dl {
  font-size: 1.2em;
}

.view-header {
  /* Too broad to correct the font size issue with view headers here. This impacts the sidebar-second. */
}

.sidebar h2, #featured {
  text-shadow: none;
}

.node-form {
  label, .fieldset-legend {
    font-size: 1.2em;
    font-weight: normal;
  }
}

input.form-submit {
  font-size: 1.2em;
  padding: 0.25em 1.2em 0.25em 1.2em;
}

a {
  &.button {
    font-size: 1.2em;
    margin: 1em 0 1em 0;
    padding: 0.25em 1.2em 0.25em 1.2em;
  }
  &.tabledrag-handle .handle {
    margin: -0.75em 0 0 1em;
    padding: 0.75em;
  }
}

.vertical-tabs ul.vertical-tabs-list .selected strong {
  font-size: 1.2em;
}

.form-item {
  margin: 1em 0 2em 0;
  .description {
    font-size: 0.9em;
    margin: 0 0 1em 0;
    color: #999;
  }
}

.field-multiple-table td {
  padding: 0 1em 0 1em;
}

.tabledrag-toggle-weight-wrapper {
  display: none;
}

fieldset .fieldset-wrapper, .fieldset-wrapper {
/*  padding: 1em 0 0 0; */
  margin: 0;
}

.container-inline-date {
  > .form-item {
    margin-bottom: 0;
  }
  .date-padding {
    padding: 1em 0 0 1em;
  }
}

.tips {
  font-size: 1em;
}

ul.tips {
  padding: 1em 0 1em 2em;
}

.text-format-wrapper .description {
  font-size: 1em;
  padding: 0.25em 0 1em 0;
}

.region-help {
  font-size: 1.2em;
  border: none;
  margin: 0;
  padding: 0;
}

.field-type-taxonomy-term-reference {
  margin: 0;
}

#field-collection-item-form {
  font-size: 1.4em;
}

h1#page-title {
  font-size: 1.7em;
  line-height: 1.2em;
}

div.messages {
  border: none;
}

.block-views {
  margin: 0 0 2em 0;
  h2! {}
}

.meta {
  font-size: 1.2em;
  color: #999;
  margin: -1em 0 1em 0;
}

.home.view-news-block .Job.Posting {
  font-size: 0.85em;
/*  background-color: rgb(240,240,240); */
  margin: 0 0 0.5em 0;
}

.home.view-news-block .view-employment .view-content {
  background-color: rgb(240,240,240);
}

.page-home {
	.block-menu-block.administrative {
		font-size: 1.2em;
		margin: 0 0 2em 0;

	li {
		float: left;
		margin: 0 auto;
		list-style-type: none;
		margin: 0 1em 0 1em;
		display: table-cell;
		vertical-align: middle; 
	}

	}
}

.page-navigation404 .block-system {
	font-size: 1.2em; 
}

#user-profile-form .form-item label {
	font-size: 1.2em;
}

.confirmation, .confirmation .form-item label {
	font-size: 1.2em;
}

.view-user-profiles table, .view-unpub table {
	font-size: 1em;
	min-width: 35%;
	width: auto;
	th, td {
	padding: 0.5em;
	}
}

#footer li a {
padding: 0 0.5em;
border-color: rgba(143, 143, 143, 0.15);
}

